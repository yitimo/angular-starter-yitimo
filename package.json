{
    "name": "angular-starter-yitimo",
    "version": "5.0.0",
    "description": "Angular starter by yitimo without angular-cli",
    "keywords": [
        "angular",
        "webpack",
        "typescript"
    ],
    "author": "yitimo <admin@yitimo.com>",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "yarn webpack-dev-server --config config/webpack.config.dev.js",
        "build": "yarn webpack --config config/webpack.config.prod.js",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
        "rimraf": "rimraf",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use Yarn instead of NPM to install dependencies. See: https://yarnpkg.com/lang/en/docs/install/')\""
    },
    "dependencies": {
        "@angular/animations": "^7.2.11",
        "@angular/cdk": "^7.3.6",
        "@angular/common": "^7.2.11",
        "@angular/compiler": "^7.2.11",
        "@angular/core": "^7.2.11",
        "@angular/forms": "^7.2.11",
        "@angular/material": "^7.3.6",
        "@angular/platform-browser": "^7.2.11",
        "@angular/platform-browser-dynamic": "^7.2.11",
        "@angular/router": "^7.2.11",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "core-js": "^3.0.0",
        "rxjs": "^6.4.0",
        "typescript": ">=3.1.1 <3.3",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-optimizer": "^0.13.7",
        "@angular/compiler-cli": "^7.2.11",
        "@ngtools/webpack": "^7.3.7",
        "@types/hammerjs": "^2.0.36",
        "@types/node": "^11.12.0",
        "@types/webpack": "^4.4.26",
        "codelyzer": "^5.0.0",
        "copy-webpack-plugin": "^5.0.2",
        "css-loader": "^2.1.1",
        "file-loader": "^3.0.1",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.11.0",
        "raw-loader": "^2.0.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "script-ext-html-webpack-plugin": "^2.1.3",
        "style-loader": "^0.23.1",
        "to-string-loader": "^1.1.5",
        "tslint": "^5.14.0",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.2.1",
        "webpack-merge": "^4.2.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yitimo/angular-starter-yitimo.git"
    },
    "bugs": {
        "url": "https://github.com/yitimo/angular-starter-yitimo/issues"
    }
}
